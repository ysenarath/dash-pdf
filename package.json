{
    "name": "dash_pdf_plus",
    "version": "0.0.4",
    "description": "A Dash component library for rendering PDF documents with enhanced features.",
    "repository": {
        "type": "git",
        "url": "git://github.com/ysenarath/dash-pdf.git"
    },
    "bugs": {
        "url": "https://github.com/ysenarath/dash-pdf/issues"
    },
    "homepage": "https://github.com/ysenarath/dash-pdf",
    "main": "build/index.js",
    "scripts": {
        "clean": "find dash_pdf_plus -type f ! -name '__init__.py' -delete",
        "start": "webpack serve --config ./webpack.serve.config.js --open",
        "validate-init": "python _validate_init.py",
        "prepublishOnly": "npm run validate-init",
        "build:js": "webpack --mode production",
        "build:js:dev": "webpack --mode development",
        "build:backends": "dash-generate-components ./src/lib/components dash_pdf_plus -p package-info.json --ignore \\.test\\.",
        "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:backends)",
        "build": "npm run clean && npm run build:js && npm run build:backends",
        "build:dev": "npm run clean && npm run build:js:dev && npm run build:backends",
        "build:activated": "npm run build:js && npm run build:backends-activated"
    },
    "author": "Yasas Senarath",
    "license": "Apache-2.0",
    "dependencies": {
        "ramda": "^0.26.1",
        "react-pdf": "^9.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.22.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/preset-env": "^7.22.2",
        "@babel/preset-react": "^7.22.3",
        "@plotly/dash-component-plugins": "^1.2.3",
        "@plotly/webpack-dash-dynamic-import": "^1.2.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^9.1.2",
        "copy-webpack-plugin": "^13.0.1",
        "copyfiles": "^2.1.1",
        "css-loader": "^6.8.1",
        "eslint": "^6.0.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-react": "^7.14.2",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-docgen": "^5.4.3",
        "react-dom": "^18.2.0",
        "style-loader": "^3.3.3",
        "styled-jsx": "^5.1.6",
        "terser-webpack-plugin": "^5.3.14",
        "webpack": "^5.84.1",
        "webpack-cli": "^5.1.1",
        "webpack-dev-server": "^4.15.0"
    },
    "engines": {
        "node": ">=8.11.0",
        "npm": ">=6.1.0"
    }
}
